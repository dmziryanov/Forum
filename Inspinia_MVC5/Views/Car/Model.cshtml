@using RmsAuto.TechDoc
@model IEnumerable<DAL.KeyNameModel>
@{
    Layout = null;
}

<!-- Select Basic -->
<div class="form-group">
    <label class="col-md-3 control-label">Модель</label>
    <div class="col-md-8">
        <select onchange="" id="CarModel" name="CarModel" class="form-control">
            <option value="0" selected="selected" disabled="disabled">Выберите... </option>
            @foreach (var subitem in @Model)
            {
                <option value="@subitem.Id" style="background-color: #E9E9E9; font-weight: bold;"> @subitem.Name </option>
            }
        </select>
    </div>
</div>